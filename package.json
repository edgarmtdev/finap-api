{
  "name": "finance-app",
  "version": "1.0.0",
  "description": "A software for finances",
  "main": "index.ts",
  "scripts": {
    "test": "cross-env NODE_ENV=test ts-node ts-mocha -p ./tsconfig.json src/test/*.spec.ts",
    "start": "cross-env NODE_ENV=production babel-node --extensions \".ts\" index.ts",
    "dev": "cross-env NODE_ENV=development nodemon --exec npx babel-node --extensions \".ts\" index.ts",
    "lint:fix": "eslint --fix .",
    "lint:check": "eslint .",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "format": "prettier --write .",
    "prisma-dev": "dotenv -e .env.development.local -- npx prisma migrate dev",
    "prisma-qa": "dotenv -e .env.test.local -- npx prisma migrate dev"
  },
  "author": "edgarmt.dev",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^4.6.1",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^8.5.9",
    "babel": "^6.23.0",
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^13.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/node": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/chai": "^4.3.4",
    "@types/cookie-parser": "^1.4.3",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^10.0.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "chai": "^4.3.7",
    "eslint": "^8.28.0",
    "husky": "^8.0.2",
    "mocha": "^10.2.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.0",
    "prisma": "^4.6.1",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
